			<!-- DynamicPage Content -->
		
		<fA:SimpleForm id="Form1" editable="false"
								layout="ResponsiveGridLayout" title="PO_NO LIST" labelSpanXL="4"
								labelSpanL="3" labelSpanM="12" labelSpanS="12"
								adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0"
								emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2"
								columnsM="1" singleContainerFullSize="false" >
								
								
								
								</fA:SimpleForm>
		<content>
		  <t:Table id ="po_no" rows="{/PoCollection}" selectionMode="Single"
				visibleRowCount="7" rowSelectionChange="changetable">
		   <t:columns>
		    <t:Column>
		     <Label text = "발주번호"/>
		      <t:template>
		       <Text text= "{poNo}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "발주일자"/>
		      <t:template>
		       <Text text= "{poDate}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "제품명"/>
		      <t:template>
		       <Text text= "{poCat3}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "공급업체"/>
		      <t:template>
		       <Text text= "{poVname}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "공급업체코드"/>
		      <t:template>
		       <Text text= "{poVcode}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "잔여수량"/>
		      <t:template>
		       <Text text= "{poTotal}"/>
		      </t:template>
		    </t:Column>
		    
		      <t:Column>
		     <Label text = "발주수량"/>
		      <t:template>
		       <Input value = "{poAmt}" valueState="{color>/Color}"  editable="false" />
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "단가"/>
		      <t:template>
		       <Text text= "{poEprice}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "총금액"/>
		      <t:template>
		       <Text text= "{poTprice}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "입고예정일"/>
		      <t:template>
		       <Text text= "{poDdate}"/>
		      </t:template>
		    </t:Column>
		    <t:Column>
		     <Label text = "결제예정일"/>
		      <t:template>
		       <Text text= "{poEtc}"/>
		      </t:template>
		    </t:Column>
		    
		   </t:columns>
		  </t:Table>
		</content>
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
