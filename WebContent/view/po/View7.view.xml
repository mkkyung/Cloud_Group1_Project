<?xml version="1.0" encoding="UTF-8"?>
<mvc:View
	controllerName="Cloud_Group1_ProjectCloud_Group1_Project.controller.po.View7"
	xmlns:core="sap.ui.core" 
	xmlns:t="sap.ui.table" xmlns="sap.m" xmlns:u="sap.uxap" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" 
	xmlns:fb="sap.ui.comp.filterbar" xmlns:f="sap.f" xmlns:fA="sap.ui.layout.form">
	
	<App id="apps">
		<Page title="view7 : 발주조회" showNavButton="true"
			navButtonPress="goBack">
			<Button text="button" press="getData" />
			<!-- <Button text="재고화면dmf 확인하자 ㅇㅅㅇ" press="viewStock"/> -->
			<headerContent>
			<Button text="next" id="button" type="Reject" press=".onShow" />
				<!-- <Button text="next" id="button" type="Reject" press=".onShow" /> -->
		   
					<Button id="update" icon="sap-icon://edit" type="Emphasized"
						press="edit" visible="{edit>/Edit}" />
					<Button id="enter" icon="sap-icon://save" type="Emphasized"
						visible="{enter>/Enter}" press="enter" />
					<Button icon="sap-icon://delete" type="Reject" visible="{edit>/Edit}"/>
					<Button icon="sap-icon://reset" type="Reject" visible="{cancel>/Cancel}" press="cancel" />
					<Button icon="sap-icon://action" type="Transparent" />
			
			
			
			</headerContent>
		
					<IconTabBar
						id="idIconTabBar"
						select="handleIconTabBarSelect"
						class="sapUiResponsiveContentPadding">
						<items>
						<IconTabFilter
								showAll="true"
								text="Products"
								key="All" >
								<fb:FilterBar id="filterbar" reset="onReset"
									search="onSearch" useToolbar="false" showGoOnFB="false" > <!-- search="onSearch" -->
									<fb:filterItems>
			
										<fb:FilterItem name="A" label="발주번호">
											<fb:control>
												<Input id="A" required="true" maxLength="11" textFormatMode="KeyValue"
				 										
														showSuggestion="true"
														showValueHelp="true"
														valueHelpRequest="handleValueHelp"
														suggestionItems="{/ProductCollection}"
														suggestionItemSelected="suggestionItemSelected">
												<suggestionItems>
												<core:ListItem key="{ProductId}" text="{Name}" additionalText="{ProductId}" />
												</suggestionItems>
												</Input>
											</fb:control>
										</fb:FilterItem>
									
										<fb:FilterItem name="B" label="발주일자">
											<fb:control>
												<DatePicker id="B"
													value="{
												path:'/dateValue', 
												type:'sap.ui.model.type.Date', 
												formatOptions: { 
													style: 'medium', 
													strictParsing: true
													}
												}"
													class="sapUiSmallMarginBottom"
													change="handleChange" />
											</fb:control>
										</fb:FilterItem>
										<fb:FilterItem name="C" label="공급업체명">
											<fb:control>
												<Input id="C" required="true" maxLength="15"/>
											</fb:control>
										</fb:FilterItem>
										
										<fb:FilterItem name="D" label="제품명" >
											<fb:control>
												<Input id="D" required="true" maxLength="15"/>
											</fb:control>
										</fb:FilterItem>
										
									</fb:filterItems>
								</fb:FilterBar>
							</IconTabFilter>	
						
						<IconTabFilter
								icon="sap-icon://begin"
								iconColor="Positive"
								text="Ok"
								key="A" >
								<fb:FilterBar id="filterbar1" reset="onReset"
									search="onSearch" useToolbar="false" showGoOnFB="false" > <!-- search="onSearch" -->
									<fb:filterItems>
			
										
										<fb:FilterItem name="A" label="발주번호">
											<fb:control>
												<Input id="A1" required="true" maxLength="11" textFormatMode="KeyValue"
				 									
														suggestionItemSelected="suggestionItemSelected">
												<suggestionItems>
												<core:ListItem key="{ProductId}" text="{Name}" additionalText="{ProductId}" />
												</suggestionItems>
												</Input>
											</fb:control>
										</fb:FilterItem>
										
										<fb:FilterItem name="B" label="발주일자">
											<fb:control>
												<DatePicker id="B1"
													value="{
												path:'/dateValue', 
												type:'sap.ui.model.type.Date', 
												formatOptions: { 
													style: 'medium', 
													strictParsing: true
													}
												}"
													class="sapUiSmallMarginBottom"
													change="handleChange" />
											</fb:control>
										</fb:FilterItem>
										<fb:FilterItem name="C" label="공급업체명">
											<fb:control>
												<Input id="C1" required="true" maxLength="15"/>
											</fb:control>
										</fb:FilterItem>
										
										<fb:FilterItem name="D" label="제품명" >
											<fb:control>
												<Input id="D1" required="true" maxLength="15"/>
											</fb:control>
										</fb:FilterItem>
										
										
									</fb:filterItems>
								</fb:FilterBar>
							</IconTabFilter>
<!-- 	Icon tabfilter 3						 -->							
							<IconTabFilter
								icon="sap-icon://compare"
								iconColor="Critical"
								text="Heavy"
								key="B" >
									
							</IconTabFilter>
							
