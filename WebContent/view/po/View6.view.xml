					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="유효성여부" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="대분류" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="중분류" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="제품명" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="공급업체명" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="공급업체코드" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="수량" />
