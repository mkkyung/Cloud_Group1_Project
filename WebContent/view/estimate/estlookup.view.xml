					<Text text="상품명" class="sapUiTinyMarginBottom" />
					<Text text="총금액" class="sapUiTinyMarginBottom" />
				</l:VerticalLayout>
				<l:VerticalLayout class="sapUiTinyMarginEnd">
					<Title level="H2" titleStyle="H2" text="공급업체"
						class="sapUiMediumMarginBottom" />
					<Text text="공급업체명" class="sapUiTinyMarginBottom" />
					<Text text="공급업체코드" class="sapUiTinyMarginBottom" />
					<Text text="사업자등록번호" class="sapUiTinyMarginBottom" />
				</l:VerticalLayout>
			</l:Grid>
			<IconTabBar expanded=""
				class="sapUiSmallMarginBottom sapUiResponsiveContentPadding"
				backgroundDesign="Solid">
				<items>
					<IconTabFilter text="견적서 상세 조회">
						<VBox class="sapUiSmallMargin">
